    "rimraf": "^6.0.1",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "^5.0.0-alpha.172",
